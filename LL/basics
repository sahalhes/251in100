#include <bits/stdc++.h>
using namespace std;

struct node(){
    int data;
    node *next;
}

int main(){
    node* head = new node();
    node* first = new node();
    node* second = new node();

    head->data=0;
    head->next=first;
    first->data=1;
    first->next=second;
    second->data=2;
    second->next=NULL;

    // 1->2-->3
}