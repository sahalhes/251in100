#include <bits/stdc++.h> 
void insertionSort(int n, vector<int> &arr){
    // Write your code here.
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            while(j>0 && arr[j-1]>arr[j]) {
                swap(arr[j-1],arr[j]);
                j--;
            }
        }
    }
}  