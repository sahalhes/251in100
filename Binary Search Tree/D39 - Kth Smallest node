//BST inorder already sorted
void inorder(TreeNode<int> *root, vector<int>&ans)

{
if(root==NULL)return;

inorder(root->left,ans);
ans.push_back(root->data);
inorder(root->right,ans);
}

int kthSmallest(TreeNode<int> *root, int k)

{
vector<int> ans;
inorder(root,ans);
return ans[k-1];

}