//Brute
int floorSqrt(int n)
{
    // Write your code here.
    int i=1;
    while(i*i<=n){
        i++;
    }
    return i-1;
}

//Sqrt Root
#include<bits/stdc++.h>

int bs(int n){
    long long int low = 1,high=n;

    long long int ans=-1;
    while(low<=high){
        long long int mid = low + (high-low)/2;
        if((long long)mid*mid<=n){
            ans = mid;
            low = mid+1;
        }else{
            high = mid-1;
        }
    }
    return ans;
}

int floorSqrt(int n)
{
    // Write your code here.
    return bs(n);
}

