#include <bits/stdc++.h> 
int findSecondLargest(int n, vector<int> &arr)
{
    // Write your code here.
    sort(arr.begin(),arr.end());
    int r = n;
    int s= arr[n-2] , t = arr[n-1],count=0;
    if(s==t){
        while(arr[r-2]==arr[r-1]){
            r--;
            count++;
            if(count>2) return -1;
        }
        s=arr[r-2];
        
    }
    return s;
}
